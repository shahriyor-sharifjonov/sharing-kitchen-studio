                    .header__dropdown-content 
                        ul.header__dropdown-list  
                            li.header__dropdown-li 
                                a(href="#!").header__dropdown-link Видео-рецепты 
                            li.header__dropdown-li 
                                a(href="#!").header__dropdown-link Онлайн курс/видео-лекция
                            li.header__dropdown-li 
                                a(href="#!").header__dropdown-link Видео-обзоры
                            li.header__dropdown-li 
                                a(href="#!").header__dropdown-link Предметная съемка
                            li.header__dropdown-li 
                                a(href="#!").header__dropdown-link Вебинары/трансляции
                li.header__li 
                    a(href="#!").header__link Оснащение студии
                li.header__li 
                    a(href="questions.html").header__link Ответы на вопросы
                li.header__li 
                    a(href="contacts.html").header__link Контакты
            a(href="tel:+84959999999").header__tel 8 (495) 999 99 99
            button.header__button 
              span.header__button-line

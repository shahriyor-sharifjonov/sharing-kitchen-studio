mixin header()
    header.header   
        .header__nav.container 
            a(href="/index.html").header__logo
                img(src="@img/logo.svg", alt="" draggable="false")
            ul.header__list 
                li.header__li 
                    a(href="#!").header__link Наша студия
                li.header__li.header__dropdown 
                    a(href="services.html").header__link.header__dropdown-button Услуги и цены
                        <svg width="33" height="32" viewBox="0 0 33 32" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M10.753 10.6665L16.873 16.7732L22.993 10.6665L24.873 12.5465L16.873 20.5465L8.87305 12.5465L10.753 10.6665Z" fill="black" fill-opacity="0.87"/>
